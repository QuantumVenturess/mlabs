<div class="pageHeader">
	<ul>
		<%= render 'side_panel' %>
	</ul>
</div>
<div class="pageContent">
	<% @jobs.each do |job| %>
		<p><%= job.name %> <%= link_to "Edit", edit_job_path(job) %> | <%= link_to "Delete", job, method: :delete, confirm: "Are you sure you want to delete this job?" %></p>
	<% end %>
</div>